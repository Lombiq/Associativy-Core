@model Associativy.FrontendEngines.ViewModels.ISearchResultViewModel
@{
    Script.Require("JIT").AtHead();

    var routeData = Html.ViewContext.RouteData.Values;
}
@Display(Model.SearchForm)
<script type="text/javascript">
$(document).ready(function(){
	$.getJSON("@Url.Action("FetchAssociations", routeData["controller"].ToString(), new { ZoomLevel = 5 })&" + $("#associativy-search-form").serialize(), function(data) {
        $.jit.draw($.parseJSON(data));
    });
});
</script>
<div id="jit-associations" style="width: 700px; height: 600px;">
</div>